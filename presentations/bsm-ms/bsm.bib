pmid = {26589277},
title = {{SomVarIUS: somatic variant identification from unpaired tissue samples}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/26589277 https://academic.oup.com/bioinformatics/article-lookup/doi/10.1093/bioinformatics/btv685},
