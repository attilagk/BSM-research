keywords = {Algorithms,Calibration,DNA,DNA: methods,DNA: standards,Genetic,Models,Molecular Probes,Nonlinear Dynamics,Oligonucleotide Array Sequence Analysis,Oligonucleotide Array Sequence Analysis: instrumen,Oligonucleotide Array Sequence Analysis: methods,Oligonucleotide Array Sequence Analysis: standards,Quality Control,Sequence Analysis,Stochastic Processes,expression,genomics,statistics,technique},
mendeley-tags = {expression,genomics,statistics,technique},
month = {jan},
