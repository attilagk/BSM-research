journal = {Nucleic Acids Research},
keywords = {Algorithms,Cluster Analysis,Computer Graphics,Databases, Genetic,Gene Expression,Gene Expression Regulation,Humans,Internet,MicroRNAs,MicroRNAs: metabolism,Polymorphism, Single Nucleotide,Software,miRNA,tool},
mendeley-tags = {miRNA,tool},
month = {jul},
number = {W1},
pages = {W498--504},
pmid = {22649059},
title = {{DIANA miRPath v.2.0: Investigating the combinatorial effect of microRNAs in pathways}},
