pages = {2843--2851},
pmid = {24974202},
title = {{Toward better understanding of artifacts in variant calling from high-coverage samples}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/24974202 https://www.ncbi.nlm.nih.gov/pmc/articles/PMC4271055/ https://academic.oup.com/bioinformatics/article/30/20/2843/2422145/Toward-better-understanding-of-artifacts-in?searchresult=1},
volume = {30},
