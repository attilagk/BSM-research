title = {{Increased power of mixed models facilitates association mapping of 10 loci for metabolic traits in an isolated population}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3024042{\&}tool=pmcentrez{\&}rendertype=abstract},
