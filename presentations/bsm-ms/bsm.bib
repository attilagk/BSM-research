file = {:home/attila/bibliography/mendeley/Raju, Owen, Neglen/Raju, Owen, Neglen - 2002 - The clinical impact of iliac venous stents in the management of chronic venous insufficiency.pdf:pdf},
isbn = {0741-5214 (Print)},
