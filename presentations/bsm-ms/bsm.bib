month = {aug},
number = {2},
pages = {174--180},
pmid = {12126879},
title = {{Comparison of spontaneous and evoked epileptiform activity in three in vitro epilepsy models}},
url = {http://www.sciencedirect.com/science/article/pii/S0006899302027518},
