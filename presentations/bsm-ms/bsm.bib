title = {{Synaptotagmin-1 Docks Secretory Vesicles to Syntaxin-1/SNAP-25 Acceptor Complexes}},
url = {http://www.sciencedirect.com/science/article/pii/S0092867409009064 http://www.cell.com/cell/abstract/S0092-8674{\%}2809{\%}2900906-4?{\_}returnURL=http{\%}3A{\%}2F{\%}2Flinkinghub.elsevier.com{\%}2Fretrieve{\%}2Fpii{\%}2FS0092867409009064{\%}3Fshowall{\%}3Dtrue},
