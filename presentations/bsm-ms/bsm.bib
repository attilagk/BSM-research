keywords = {Algorithms,Animals,Bayes Theorem,Brain,Brain: metabolism,Cadherins,Cadherins: metabolism,High-Throughput Screening Assays,Humans,Matrix Attachment Region Binding Proteins,Matrix Attachment Region Binding Proteins: metabol,Mice,Models,Molecular,PPAR gamma,PPAR gamma: metabolism,Phylogeny,Protein Binding,Protein Conformation,Protein Interaction Mapping,Protein Interaction Mapping: methods,Protein Interaction Maps,Protein Kinases,Protein Kinases: chemistry,Protein Kinases: metabolism,Proteins,Proteins: chemistry,Proteins: metabolism,Proteome,Proteome: chemistry,Proteome: metabolism,Proteomics,Proteomics: methods,ROC Curve,Reproducibility of Results,Saccharomyces cerevisiae,Saccharomyces cerevisiae: chemistry,Saccharomyces cerevisiae: metabolism,Suppressor of Cytokine Signaling Proteins,Suppressor of Cytokine Signaling Proteins: metabol,Transcription Factors,Transcription Factors: metabolism,machine{\_}learning,protein},
mendeley-tags = {machine{\_}learning,protein},
