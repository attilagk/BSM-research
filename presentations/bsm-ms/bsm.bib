pages = {3300--5},
pmid = {17360642},
title = {{Human polymorphism at microRNAs and microRNA target sites.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1805605{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {104},
