@article{Gulyas-Kovacs:2006,
abstract = {SM proteins are essential elements in all mammalian fusion pathways. The yeast exocytotic SM protein sec-1 binds to the assembled SNARE complex, and may speed up SNARE complex formation. In contrast, the vertebrate exocytotic SM protein, Munc18-1, binds to syntaxin 1 in a conformation which is incompatible with SNARE complex formation. The physiological function of this interaction is controversial. We have investigated the function of syntaxin-binding by overexpression of Munc18-1 constructs in munc18-1 null mouse chromaffin cells. In these cells, docking of secretory vesicles to the plasma membrane is impaired. Different Munc18-1 mutants (D34N/M38V and S313D) with reduced binding to syntaxin led to a partial restoration of the morphological docked vesicle pool. Electrophysiological data showed that the docked vesicles were fully functional, indicating intact priming and fusion reactions downstream of docking. Next, we evaluated whether Munc18 proteins might have roles downstream of docking, by overexpressing the Munc18-2 isoform in munc18-1 null cells. Munc18-2 binds strongly to syntaxin 1, but is expressed and functions together with syntaxin 3 in other cell types. We found that Munc18-2 could fully rescue the morphologically docked pool in munc18-1 null chromaffin cells but secretion was only restored to {\~{}}30{\%} of the Munc18-1 level. Kinetic analysis indicated that the fusion triggering reaction was normal in the presence of Munc18-2, identifying a Munc18-1-dependent step between docking and fusion of vesicles, i.e. at the priming stage. At this step Munc18-2 can only partially substitute for Munc18-1, even though docking is restored. These results indicate that the syntaxin-dependent function of Munc18-1 correlates with vesicle docking, and is located upstream of a second function of Munc18-1 in the vesicle priming reaction.},
author = {Guly{\'{a}}s-Kov{\'{a}}cs, Attila and de Wit, Heidi and Kochubey, Olexiy and Toonen, Ruud F and Klingauf, J{\"{u}}rgen and Verhage, Matthijs and S{\o}rensen, Jakob B},
journal = {2006 Biophysical Society Meeting Abstracts, Biophysical Journal, Supplement},
title = {{Dissection of the roles of Munc18-1 in the secretory cascade}},
year = {2006}
}
