keywords = {Artificial Intelligence,Bayes Theorem,Bayesian,Cognition,Concept Formation,Humans,Knowledge,Learning,Models,Statistical,Theory of Mind,Thinking,machine{\_}learning,review},
mendeley-tags = {Bayesian,machine{\_}learning,review},
month = {mar},
