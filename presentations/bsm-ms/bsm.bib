journal = {PLoS computational biology},
keywords = {DNA Methylation,Databases, Genetic,Female,Gene Expression Profiling,Genomics,Genomics: methods,Humans,Male,Molecular Sequence Annotation,Molecular Sequence Annotation: methods,Neoplasms,Neoplasms: genetics,Polymorphism, Single Nucleotide,Sequence Analysis, DNA,Sequence Analysis, RNA,expression,genomics,tool},
mendeley-tags = {expression,genomics,tool},
month = {aug},
number = {8},
pages = {e1003790},
pmid = {25122495},
publisher = {Public Library of Science},
title = {{MODMatcher: multi-omics data matcher for integrative genomic analysis.}},
url = {http://journals.plos.org/ploscompbiol/article?id=10.1371/journal.pcbi.1003790},
volume = {10},
year = {2014}
}
