title = {{AbCD: arbitrary coverage design for sequencing-based genetic studies.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23357921 https://www.ncbi.nlm.nih.gov/pmc/articles/PMC3597143/},
volume = {29},
