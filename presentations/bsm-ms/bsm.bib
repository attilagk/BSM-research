volume = {426},
year = {2014}
}
@article{Janzing2012,
abstract = {While conventional approaches to causal inference are mainly based on conditional (in)dependences, recent methods also account for the shape of (conditional) distributions. The idea is that the causal hypothesis "X causes Y" imposes that the marginal distribution PX and the conditional distribution PY| X represent independent mechanisms of nature. Recently it has been postulated that the shortest description of the joint distribution PX, Y should therefore be given by separate descriptions of PX and PY| X. Since description length in the sense of Kolmogorov complexity is uncomputable, practical implementations rely on other notions of independence. Here we define independence via orthogonality in information space. This way, we can explicitly describe the kind of dependence that occurs between PY and PX| Y making the causal hypothesis "Y causes X" implausible. Remarkably, this asymmetry between cause and effect becomes particularly simple if X and Y are deterministically related. We present an inference method that works in this case. We also discuss some theoretical results for the non-deterministic case although it is not clear how to employ them for a more general inference method. ?? 2012 Elsevier B.V. All rights reserved.},
author = {Janzing, Dominik and Mooij, Joris and Zhang, Kun and Lemeire, Jan and Zscheischler, Jakob and Daniu??is, Povilas and Steudel, Bastian and Sch??lkopf, Bernhard},
doi = {10.1016/j.artint.2012.01.002},
issn = {00043702},
journal = {Artificial Intelligence},
keywords = {Cause-effect pairs,Deterministic causal relations,Pythagorean triple,machine{\_}learning,statistics},
mendeley-tags = {machine{\_}learning,statistics},
month = {may},
pages = {1--31},
title = {{Information-geometric approach to inferring causal directions}},
url = {http://www.sciencedirect.com/science/article/pii/S0004370212000045},
volume = {182-183},
year = {2012}
}

@InProceedings{Jaksekovic:2008,
  author        = {Jak{\v{s}}ekovi{\'{c}}, Inna and Guly{\'{a}}s-Kov{\'{a}}cs, Attila and Patel, Ankit B and Ram, Rupa and Gadsby, David C},
  title         = {{Role of F508 and L1065 in the interaction between NBD1 and intracellular loop 4 of CFTR}},
  booktitle     = {2008 Biophysical Society Meeting Abstracts, Biophysical Journal, Supplement},
  year          = {2008},
  abstract      = {CFTR, the chloride channel dysfunctional in cystic fibrosis, belongs to the exporter class of ABC transporters. These proteins contain two nucleotide binding domains (NBDs) and two transmembrane domains (TMDs), and each TMD incorporates two long intracellular loops (ICLs). X-ray structures of Sav1866 and MsbA, bacterial members of this class of ABC proteins, show strong interaction between each NBD and the C-terminal ICL of the `opposite' TMD. Our homology model of CFTR based on Sav1866 reveals a tight hydrophobic interface between NBD1 and ICL4 that includes F508 (NBD1), whose deletion causes misfolding and underlies most cases of cystic fibrosis. Statistical coupling analysis of ABC protein sequences predicts that F508 is part of a conserved network of energetically connected positions. The same network contains ICL4 residue L1065, which is found adjacent to F508 in the structural model, implying that these residues functionally link NBD1 to ICL4. In oocytes, F508D, F508R or L1065D mutations strongly attenuated forskolin-induced membrane conductance, consistent with charge introduction disrupting a hydrophobic interface. In contrast, even partially cysteine-depleted split (after 633) CFTR containing both F508C and L1065C still generated substantial forskolin-induced conductance. However, BMOE, a short cysteine-specific crosslinker reduced that conductance, indicating that crosslinking between F508C and L1065C trapped channels in a closed conformation. Indeed, Western blots of oocyte membranes containing split CFTR that included both F508C and L1065C mutations revealed a BMOE-induced crosslinked product with reduced mobility. These results suggest that CFTR's F508 and L1065 represent an evolutionarily conserved interaction pair at the NBD1-ICL4 interface in the exporter class of ABC transporters, and that this interaction is required for maturation and ion channel function of CFTR.
Supported by NIH DK 51767.},
  keywords      = {evolution},
  mendeley-tags = {evolution},
}
@article{Picollo:2009fk,
abstract = {Ion binding to secondary active transporters triggers a cascade of conformational rearrangements resulting in substrate translocation across cellular membranes. Despite the fundamental role of this step, direct measurements of binding to transporters are rare. We investigated ion binding and selectivity in CLC-ec1, a H(+)-Cl(-) exchanger of the CLC family of channels and transporters. Cl(-) affinity depends on the conformation of the protein: it is highest with the extracellular gate removed and weakens as the transporter adopts the occluded configuration and with the intracellular gate removed. The central ion-binding site determines selectivity in CLC transporters and channels. A serine-to-proline substitution at this site confers NO(3)(-) selectivity upon the Cl(-)-specific CLC-ec1 transporter and CLC-0 channel. We propose that CLC-ec1 operates through an affinity-switch mechanism and that the bases of substrate specificity are conserved in the CLC channels and transporters.},
author = {Picollo, Alessandra and Malvezzi, Mattia and Houtman, Jon C D and Accardi, Alessio},
doi = {10.1038/nsmb.1704},
journal = {Nat Struct Mol Biol},
month = {dec},
