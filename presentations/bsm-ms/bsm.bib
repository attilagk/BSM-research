mendeley-tags = {miRNA,prediction},
month = {may},
number = {5},
pages = {495--500},
pmid = {15806104},
shorttitle = {Nat Genet},
title = {{Combinatorial microRNA target predictions.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/15806104},
volume = {37},
year = {2005}
}
