title = {{Macromolecular modeling with rosetta.}},
url = {http://www.annualreviews.org/doi/full/10.1146/annurev.biochem.77.062906.171838?url{\_}ver=Z39.88-2003{\&}rfr{\_}id=ori:rid:crossref.org{\&}rfr{\_}dat=cr{\_}pub=pubmed http://www.ncbi.nlm.nih.gov/pubmed/18410248},
volume = {77},
