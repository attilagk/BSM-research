mendeley-tags = {expression,machine{\_}learning,miRNA,prediction},
month = {jan},
number = {JAN},
pages = {103},
pmid = {22303397},
publisher = {Frontiers},
title = {{Accurate microRNA target prediction using detailed binding site accessibility and machine learning on proteomics data}},
url = {http://www.frontiersin.org/Journal/10.3389/fgene.2011.00103/abstract},
volume = {2},
year = {2012}
}

