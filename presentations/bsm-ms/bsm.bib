  issn-linking    = {0002-953X},
  issue           = {10},
  keywords        = {Delivery of Health Care, statistics & numerical data; Female; Genetic Pleiotropy, genetics; Genetic Predisposition to Disease, genetics; Humans; Male; Middle Aged; Multifactorial Inheritance, genetics; Penetrance; Risk Factors; Schizophrenia, genetics; Chronic Psychiatric Illness; Cognitive Neuroscience; Genetics; Schizophrenia},
  mid             = {NIHMS1061044},
  nlm-id          = {0370512},
