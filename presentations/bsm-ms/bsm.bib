pages = {485},
pmid = {19843339},
title = {{The theory of discovering rare variants via DNA sequencing}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/19843339 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC2778663 http://bmcgenomics.biomedcentral.com/articles/10.1186/1471-2164-10-485},
