journal = {Current Medicinal Chemistry},
keywords = {Drug Discovery,Drug Discovery: methods,Drug Repositioning,Humans,Information Storage and Retrieval,Information Storage and Retrieval: methods,Support Vector Machines,machine{\_}learning},
mendeley-tags = {machine{\_}learning},
