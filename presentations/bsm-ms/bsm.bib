title = {{multiSNV: a probabilistic approach for improving detection of somatic point mutations from multiple related tumour samples}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/25722372 https://www.ncbi.nlm.nih.gov/pmc/articles/PMC4482059/ https://academic.oup.com/nar/article-lookup/doi/10.1093/nar/gkv135},
