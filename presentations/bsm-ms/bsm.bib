journal = {BMC bioinformatics},
keywords = {Algorithms,Computational Biology,Computational Biology: methods,Knowledge Bases,Lipid Bilayers,Lipid Bilayers: chemistry,Membrane Potentials,Membrane Potentials: physiology,Membrane Proteins,Membrane Proteins: chemistry,Models, Molecular,Reproducibility of Results,prediction,protein,technique},
mendeley-tags = {prediction,protein,technique},
month = {jan},
number = {1},
pages = {276},
pmid = {24047460},
title = {{Membrane protein orientation and refinement using a knowledge-based statistical potential.}},
url = {http://www.biomedcentral.com/1471-2105/14/276},
volume = {14},
year = {2013}
}
