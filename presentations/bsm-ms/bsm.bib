journal = {PLoS ONE},
keywords = {Algorithms,Applied mathematics,Bayes theorem,Biochemistry,Biology and life sciences,Biophysics,Biostatistics,Complex systems,Computational biology,Computer and information sciences,Evolutionary modeling,Functional genomics,Genetics,Genomics,Mathematics,Molecular biology,Molecular biology techniques,Physical sciences,Physics,Probability theory,Protein folding,Protein interactions,Protein structure,Protein structure prediction,Proteins,Proteomic databases,Proteomics,Research Article,Sequence analysis,Sequencing techniques,Statistical mechanics,Statistics (mathematics),Structural genomics,Systems biology,Systems science,Theoretical biology,evolution,protein},
mendeley-tags = {evolution,protein},
