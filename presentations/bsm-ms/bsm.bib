pages = {e1004319},
pmid = {24787348},
title = {{Allele-specific genome-wide profiling in human primary erythroblasts reveal replication program organization.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/24787348 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4006724 http://journals.plos.org/plosgenetics/article?id=10.1371/journal.pgen.1004319},
