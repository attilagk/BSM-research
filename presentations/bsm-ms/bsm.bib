  keywords        = {Disease, genetics; Drug Delivery Systems; Gene Regulatory Networks; Genome, Human; Genome-Wide Association Study; Humans; Pharmaceutical Preparations, chemistry, metabolism; Polymorphism, Single Nucleotide},
  nlm-id          = {100965258},
  owner           = {NLM},
  pii             = {1471-2164-15-S4-S5},
  pmc             = {PMC4083410},
  pmid            = {25057111},
