@article{Madigan1994,
abstract = {We consider the problem of model selection and accounting for model$\backslash$nuncertainty in high-dimensional contingency tables, motivated by expert$\backslash$nsystem applications. The approach most used currently is a stepwise$\backslash$nstrategy guided by tests based on approximate asymptotic P values$\backslash$nleading to the selection of a single model; inference is then$\backslash$nconditional on the selected model. The sampling properties of such a$\backslash$nstrategy are complex, and the failure to take account of model$\backslash$nuncertainty leads to underestimation of uncertainty about quantities of$\backslash$ninterest. In principle, a panacea is provided by the standard Bayesian$\backslash$nformalism that averages the posterior distributions of the quantity of$\backslash$ninterest under each of the models, weighted by their posterior model$\backslash$nprobabilities. Furthermore, this approach is optimal in the sense of$\backslash$nmaximizing predictive ability. But this has not been used in practice,$\backslash$nbecause computing the posterior model probabilities is hard and the$\backslash$nnumber of models is very large (often greater than 10(11)). We argue$\backslash$nthat the standard Bayesian formalism is unsatisfactory and propose an$\backslash$nalternative Bayesian approach that, we contend, takes full account of$\backslash$nthe true model uncertainty by averaging over a much smaller set of$\backslash$nmodels. An efficient search algorithm is developed for finding these$\backslash$nmodels. We consider two classes of graphical models that arise in expert$\backslash$nsystems: the recursive causal models and the decomposable log-linear$\backslash$nmodels. For each of these, we develop efficient ways of computing exact$\backslash$nBayes factors and hence posterior model probabilities. For the$\backslash$ndecomposable log-linear models, this is based on properties of chordal$\backslash$ngraphs and hyper-Markov prior distributions and the resultant$\backslash$ncalculations can be carried out locally. The end product is an overall$\backslash$nstrategy for model selection and accounting for model uncertainty that$\backslash$nsearches efficiently through the very large classes of models involved.$\backslash$nThree examples are given. The first two concern data sets that have been$\backslash$nanalyzed by several authors in the context of model selection. The third$\backslash$naddresses a urological diagnostic problem. In each example, our model$\backslash$naveraging approach provides better out-of-sample predictive performance$\backslash$nthan any single model that might reasonably have been selected.},
author = {MADIGAN, D and RAFTERY, A E},
doi = {10.2307/2291017},
file = {:home/attila/bibliography/mendeley/MADIGAN, RAFTERY/MADIGAN, RAFTERY - 1994 - Model Selection and Accounting for Model Uncertainty in Graphical Models Using Occams Window.pdf:pdf},
isbn = {0162-1459},
issn = {0162-1459},
journal = {Journal of the American Statistical Association},
keywords = {Bayesian,chordal graph,computation,contingency,decomposable log-linear,expert system,machine{\_}learning,model,statistics,table},
mendeley-tags = {Bayesian,computation,machine{\_}learning,statistics},
month = {dec},
number = {428},
pages = {1535--1546},
publisher = {Taylor {\&} Francis},
title = {{Model Selection and Accounting for Model Uncertainty in Graphical Models Using Occams Window}},
url = {http://www.tandfonline.com/doi/abs/10.1080/01621459.1994.10476894},
