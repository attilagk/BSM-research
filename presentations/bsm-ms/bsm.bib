pages = {89--105},
pmid = {24211365},
publisher = {American Society for Investigative Pathology and the Association for Molecular Pathology},
title = {{Validation of a next-generation sequencing assay for clinical molecular oncology}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/24211365},
volume = {16},
year = {2014}
}

@Article{Paquola2016,
  author        = {Paquola, Apu{\~{a}} C.M. and Erwin, Jennifer A. and Gage, Fred H.},
  title         = {{Insights into the role of somatic mosaicism in the brain}},
  journal       = {Current Opinion in Systems Biology},
  year          = {2016},
  issn          = {24523100},
  abstract      = {Somatic mosaicism refers to the fact that cells within an organism have different genomes. It is now clear that somatic mosaicism occurs in all brains and that somatic mutations in a subset of cells can cause various rare neurodevelopmental disorders. However, for most individuals, the extent and consequences of somatic mosaicism are largely unknown. The complexity and unique features of the brain suggest that somatic mosaicism can play an important role in behavior and cognition. Here we review recent manuscripts showing instances of somatic mosaicism in the brain and estimating its extent and possible biological consequences. The consequences of somatic mosaicism span vast dimensions -from a single-locus variant, to genes and gene networks, to cells, to the interactions of the mosaic cells via neural networks affecting behavior and cognition. We highlight how systems biology approaches are particularly well suited for the complex emerging field of brain somatic mosaicism.},
  doi           = {10.1016/j.coisb.2016.12.004},
  file          = {:/home/attila/bibliography/mendeley/Paquola, Erwin, Gage/Paquola, Erwin, Gage - 2016 - Insights into the role of somatic mosaicism in the brain(2).pdf:PDF},
  keywords      = {review,somatic{\_}mosaicism},
  mendeley-tags = {review,somatic{\_}mosaicism},
}
@article{Liu2012,
abstract = {BACKGROUND: Numerous single nucleotide polymorphisms (SNPs) associated with complex diseases have been identified by genome-wide association studies (GWAS) and expression quantitative trait loci (eQTLs) studies. However, few of these SNPs have explicit biological functions. Recent studies indicated that the SNPs within the 3'UTR regions of susceptibility genes could affect complex traits/diseases by affecting the function of miRNAs. These 3'UTR SNPs are functional candidates and therefore of interest to GWAS and eQTL researchers.$\backslash$n$\backslash$nDESCRIPTION: We developed a publicly available online database, MirSNP (http://cmbi.bjmu.edu.cn/mirsnp), which is a collection of human SNPs in predicted miRNA-mRNA binding sites. We identified 414,510 SNPs that might affect miRNA-mRNA binding. Annotations were added to these SNPs to predict whether a SNP within the target site would decrease/break or enhance/create an miRNA-mRNA binding site. By applying MirSNP database to three brain eQTL data sets, we identified four unreported SNPs (rs3087822, rs13042, rs1058381, and rs1058398), which might affect miRNA binding and thus affect the expression of their host genes in the brain. We also applied the MirSNP database to our GWAS for schizophrenia: seven predicted miRNA-related SNPs (p{\textless}0.0001) were found in the schizophrenia GWAS. Our findings identified the possible functions of these SNP loci, and provide the basis for subsequent functional research.$\backslash$n$\backslash$nCONCLUSION: MirSNP could identify the putative miRNA-related SNPs from GWAS and eQTLs researches and provide the direction for subsequent functional researches.},
author = {Liu, Chenxing and Zhang, Fuquan and Li, Tingting and Lu, Ming and Wang, Lifang and Yue, Weihua and Zhang, Dai},
doi = {10.1186/1471-2164-13-661},
isbn = {1471-2164},
issn = {1471-2164},
journal = {BMC genomics},
keywords = {3' Untranslated Regions,3' Untranslated Regions: genetics,Binding Sites,Binding Sites: genetics,Databases, Genetic,Genome, Human,Genome, Human: genetics,Genome-Wide Association Study,Humans,Internet,MicroRNAs,MicroRNAs: genetics,Polymorphism, Single Nucleotide,Quantitative Trait Loci,RNA, Messenger,RNA, Messenger: genetics,Schizophrenia,Schizophrenia: genetics,expression,miRNA,prediction},
mendeley-tags = {expression,miRNA,prediction},
month = {jan},
number = {1},
pages = {661},
pmid = {23173617},
title = {{MirSNP, a database of polymorphisms altering miRNA target sites, identifies miRNA-related SNPs in GWAS SNPs and eQTLs.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3582533{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {13},
