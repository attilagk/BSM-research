  country         = {United States},
  doi             = {10.1093/gigascience/giz082},
  issn-linking    = {2047-217X},
  issue           = {7},
  keywords        = {Animals; Big Data; Genome-Wide Association Study, methods; Humans; Multifactorial Inheritance; Quantitative Trait Loci; Software; GWAS; imputation; polygenic risk score},
  nlm-id          = {101596872},
  owner           = {NLM},
  pii             = {giz082},
  pmc             = {PMC6629542},
  pmid            = {31307061},
