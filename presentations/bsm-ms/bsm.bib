issn = {1936-1238},
journal = {Annual review of biophysics},
keywords = {Animals,Biological Transport,Cell Membrane,Cell Membrane: metabolism,Humans,Models,Molecular,Molecular Biology,Molecular Biology: instrumentation,Molecular Biology: methods,Molecular Dynamics Simulation,Protein Conformation,Protein Folding,Proteins,Proteins: chemistry,Proteins: metabolism,protein,review,technique},
