mendeley-tags = {genomics,miRNA,prediction},
number = {1},
pages = {140--153},
title = {{Genome-wide Dissection of MicroRNA Functionsand Cotargeting Networks Using Gene Set Signatures}},
url = {http://www.sciencedirect.com/science/article/pii/S1097276510002418},
volume = {38},
