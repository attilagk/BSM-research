@article{Lehmann2000,
abstract = {Objective: To develop a model for Bayesian communication to enable$\backslash$nreaders to make reported data more relevant by including their prior$\backslash$nknowledge and values.$\backslash$nBackground: To change their practice, clinicians need good evidence, yet$\backslash$nthey also need to make new technology applicable to their local$\backslash$nknowledge and circumstances. Availability of the Web has the potential$\backslash$nfor greatly affecting the scientific communication process between$\backslash$nrescarch and clinician. Going beyond format changes and hyperlinking,$\backslash$nBayesian communication enables readers to make reported data more$\backslash$nrelevant by including their prior knowledge and values. This paper$\backslash$naddresses the needs and implications for Bayesian communication.$\backslash$nFormulation: Literature review and development of specifications from$\backslash$nreaders', authors', publishers', and computers' perspectives consistent$\backslash$nwith formal requirements for Bayesian reasoning.$\backslash$nResults: Seventeen specifications were developed, which included eight$\backslash$nfur readers (express prior knowledge, view effect size and variability,$\backslash$nexpress threshold, make inferences, view explanation, evaluate study and$\backslash$nstatistical quality, synthesize multiple studies, and view prior beliefs$\backslash$nof the community), three for authors (protect the author's investment,$\backslash$npublish enough information, make authoring easy), three for publishers$\backslash$n(limit liability, scale up, and establish a business model), and two for$\backslash$ncomputers (incorporate into reading process, use familiar interface$\backslash$nmetaphors). A sample client-only prototype is available at$\backslash$nhttp://omie.med.jhmi.edu/bayes.$\backslash$nConclusion: Bayesian communication has formal justification consistent$\backslash$nwith the needs of readers and can best be implemented in an online$\backslash$nenvironment. Much research must be done to establish whether the$\backslash$nformalism and the reality of readers' needs can meet.},
author = {Lehmann, H P and Goodman, S N},
doi = {10.1136/jamia.2000.0070254},
isbn = {1067-5027 (Print) 1067-5027 (Linking)},
issn = {1067-5027},
journal = {Journal of the American Medical Informatics Association},
keywords = {Bayes Theorem,Bayesian,Communication,Diffusion of Innovation,Evidence-Based Medicine,Information Theory,Internet,Publishing,Research,review,statistics},
language = {en},
mendeley-tags = {Bayesian,review,statistics},
month = {may},
number = {3},
pages = {254--266},
pmid = {10833162},
publisher = {BMJ Publishing Group Ltd},
title = {{Bayesian communication: A clinically significant paradigm for electronic publication}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=61428{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {7},
