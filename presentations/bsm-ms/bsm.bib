pages = {149--84},
pmid = {19555289},
publisher = {NIH Public Access},
title = {{The glial nature of embryonic and adult neural stem cells.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/19555289 https://www.ncbi.nlm.nih.gov/pmc/articles/PMC3086722/},
