@article{Dutheil:2005,
abstract = {We present a new method for detecting coevolving sites in molecules. The method relies on a set of aligned sequences (nucleic acid or protein) and uses Markov models of evolution to map the substitutions that occurred at each site onto the branches of the underlying phylogenetic tree. This mapping takes into account the uncertainty over ancestral states and among-site rate variation. We then build, for each site, a "substitution vector" containing the posterior estimates of the number of substitutions in each branch. The amount of coevolution for a pair of sites is then measured as the Pearson correlation coefficient between the two corresponding substitution vectors and compared to the expectation under the null hypothesis of independence. We applied the method to a 79-species bacterial ribosomal RNA data set, for which extensive structural characterization has been done over the last 30 years. More than 95{\%} of the intramolecular predicted pairs of sites correspond to known interacting site pairs.},
author = {Dutheil, Julien and Pupko, Tal and Jean-Marie, Alain and Galtier, Nicolas},
doi = {10.1093/molbev/msi183},
file = {:home/attila/bibliography/mendeley/Dutheil et al/Dutheil et al. - 2005 - A model-based approach for detecting coevolving positions in a molecule.pdf:pdf},
isbn = {0737-4038 (Print) 0737-4038 (Linking)},
issn = {07374038},
journal = {Molecular Biology and Evolution},
keywords = {Coevolution,Correlated substitutions,Intramolecular and intermolecular interaction,Markov models,RNA structure prediction,evolution},
mendeley-tags = {evolution},
month = {sep},
number = {9},
pages = {1919--1928},
pmid = {15944445},
title = {{A model-based approach for detecting coevolving positions in a molecule}},
url = {http://mbe.oxfordjournals.org/content/22/9/1919.full},
volume = {22},
year = {2005}
}
