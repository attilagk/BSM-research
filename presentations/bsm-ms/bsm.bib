journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Biophysics,Biophysics: methods,Carbon,Carbon: chemistry,Concanavalin A,Concanavalin A: chemistry,Kinetics,Models, Molecular,Models, Statistical,Monte Carlo Method,Protein Conformation,Protein Folding,Protein Structure, Tertiary,Staphylococcus aureus,Staphylococcus aureus: metabolism,Temperature,Time Factors,dynamics,protein},
mendeley-tags = {dynamics,protein},
month = {apr},
number = {8},
