title = {{Using evolutionary trees in protein secondary structure prediction and other comparative sequence analyses}},
volume = {263},
year = {1996}
}
@article{Szulwach2015,
abstract = {Somatic mosaicism occurs throughout normal development and contributes to numerous disease etiologies, including tumorigenesis and neurological disorders. Intratumor genetic heterogeneity is inherent to many cancers, creating challenges for effective treatments. Unfortunately, analysis of bulk DNA masks subclonal phylogenetic architectures created by the acquisition and distribution of somatic mutations amongst cells. As a result, single-cell genetic analysis is becoming recognized as vital for accurately characterizing cancers. Despite this, methods for single-cell genetics are lacking. Here we present an automated microfluidic workflow enabling efficient cell capture, lysis, and whole genome amplification (WGA). We find that {\~{}}90{\%} of the genome is accessible in single cells with improved uniformity relative to current single-cell WGA methods. Allelic dropout (ADO) rates were limited to 13.75{\%} and variant false discovery rates (SNV FDR) were 4.11x10(-6), on average. Application to ER-/PR-/HER2+ breast cancer cells and matched normal controls identified novel mutations that arose in a subpopulation of cells and effectively resolved the segregation of known cancer-related mutations with single-cell resolution. Finally, we demonstrate effective cell classification using mutation profiles with 10X average exome coverage depth per cell. Our data demonstrate an efficient automated microfluidic platform for single-cell WGA that enables the resolution of somatic mutation patterns in single cells.},
author = {Szulwach, Keith E and Chen, Peilin and Wang, Xiaohui and Wang, Jing and Weaver, Lesley S and Gonzales, Michael L and Sun, Gang and Unger, Marc A and Ramakrishnan, Ramesh},
doi = {10.1371/journal.pone.0135007},
issn = {1932-6203},
journal = {PloS one},
keywords = {Breast Neoplasms,Breast Neoplasms: genetics,Breast Neoplasms: pathology,Cell Line,DNA Copy Number Variations,DNA Copy Number Variations: genetics,Exome,Female,Genetic Heterogeneity,Genome,High-Throughput Nucleotide Sequencing,Human,Humans,Microfluidics,Microfluidics: methods,Mosaicism,Mutation,Single-Cell Analysis,Tumor,genomics,somatic{\_}mosaicism,technique},
mendeley-tags = {genomics,somatic{\_}mosaicism,technique},
