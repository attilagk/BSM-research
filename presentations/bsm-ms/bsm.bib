shorttitle = {Nat Neurosci},
title = {{Mapping DNA methylation across development, genotype and schizophrenia in the human frontal cortex.}},
url = {http://www.nature.com/neuro/journal/v19/n1/full/nn.4181.html?WT.ec{\_}id=NEURO-201601{\&}spMailingID=50354006{\&}spUserID=MzA2NjIxMzMxNjMS1{\&}spJobID=824237578{\&}spReportId=ODI0MjM3NTc4S0},
