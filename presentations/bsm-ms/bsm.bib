pages = {259--269},
pmid = {16551582},
title = {{Inferring complex DNA substitution processes on phylogenies using uniformization and data augmentation.}},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed{\&}id=16551582{\&}retmode=ref{\&}cmd=prlinks{\%}5Cnpapers2://publication/doi/10.1080/10635150500541599},
