keywords = {Algorithms,Cluster Analysis,Computational Biology,Computational Biology: methods,Databases, Nucleic Acid,Forecasting,Forecasting: methods,Genome, Human,Humans,Metabolic Networks and Pathways,Metabolic Networks and Pathways: genetics,Metabolic Networks and Pathways: physiology,MicroRNAs,MicroRNAs: genetics,MicroRNAs: metabolism,Models, Biological,Protein Interaction Mapping,Sequence Analysis, RNA,Sequence Analysis, RNA: methods,Signal Transduction,Signal Transduction: genetics,Software,Validation Studies as Topic,miRNA,prediction,tool},
