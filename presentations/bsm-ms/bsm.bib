@InProceedings{Gulyas-Kovacs:2009,
  author        = {Guly{\'{a}}s-Kov{\'{a}}cs, Attila and Gadsby, David C},
  title         = {{Detection of Co-evolving Positions in ABC transporters}},
  booktitle     = {2009 Biophysical Society Meeting Abstracts, Biophysical Journal, Supplement},
  year          = {2009},
  abstract      = {In proteins, amino acid positions under mutual structural or functional constraints are expected to co-evolve. Identifying co-evolving positions might thus reveal structural and mechanistic information. Correlated patterns between columns in multiple sequence alignments afford a co-evolutionary signal, which however is both obscured by spurious phylogenetic correlations since sequences share evolutionary history, and distorted by incorrect assumptions about molecular evolution. In benchmark studies here we use two contemporary methods that show improved detection of co-evolutionary signals. One, MIp (Bioinformatics 24(3): 333-40), calculates a statistic based on mutual information between position pairs. The other, CAPS (Genetics 173(1): 9-23), unlike MIp, considers physicochemical information using substitution matrices and computes, for each position pair, Pearson's correlation coefficient after pairwise sequence-sequence comparisons. Both methods tend to minimize phylogenetic and entropic (amino acid variability at a position) signals and to identify spatially proximal pairs in 3D structures of proteins suggesting that co-evolving positions are often in physical contact.

We used these two methods to detect co-evolving positions in the Organic Anion and Drug Transporter subfamily of the ATP-binding cassette transporter protein superfamily, exemplified by CFTR (Cystic Fibrosis Transmembrane Conductance Regulator) and MRP1 (Multidrug Resistance-associated Protein 1). Inclusion of very similar sequences in the multiple sequence alignment had little effect on MIp's performance whereas it introduced large number of non-significant correlations for CAPS diminishing its power to predict contacting pairs. Considering all position pairs, results by MIp show a small but significant correlation with those by CAPS, suggesting some consistency between these techniques. We identified some pairs in spatial proximity in which two amino acids of distinct physicochemical character swapped positions several times during evolution. Some identified pairs lie on separate domains or separate transmembrane helices and thus possibly stabilize specific conformations during the catalytic cycle.

[Supported by DK 51767]},
  keywords      = {evolution,prediction},
  mendeley-tags = {evolution,prediction},
}
@article{Gerstein:1994,
abstract = {We have determined the variations in volume that occur during evolution in the buried core of three different families of proteins. The variation of the whole core is very small (approximately 2.5{\%}) compared to the variation at individual sites (approximately 13{\%}). However, by comparing our results to those expected from random sequences with no correlations between sites, we show that the small variation observed may simply be a manifestation of the statistical "law of large numbers" and not reflect any compensating changes in, or global constraints upon, protein sequences. We have also analysed in detail the volume variations at individual sites, both in the core and on the surface, and compared these variations with those expected from random sequences. Individual sites on the surface have nearly the same variation as random sequences (24{\%} versus 28{\%} variation). However, individual sites in the core have about half the variation of random sequences (13{\%} versus 30{\%}). Roughly, half of these core sites strongly conserve their volume (0 to 10{\%} variation); one quarter have moderate variation (10 to 20{\%}); and the remaining quarter vary randomly (20 to 40{\%}). Our results have clear implications for the relationship between protein sequence and structure. For our analysis, we have developed a new and simple method for weighting protein sequences to correct for unequal representation, which we describe in an Appendix.},
author = {Gerstein, M and Sonnhammer, E L and Chothia, C},
journal = {J Mol Biol},
