mendeley-tags = {evolution,expression,protein},
month = {jan},
number = {1},
pages = {e1002865},
pmid = {23300434},
title = {{The Underlying Molecular and Network Level Mechanisms in the Evolution of Robustness in Gene Regulatory Networks}},
url = {http://journals.plos.org/ploscompbiol/article?id=10.1371/journal.pcbi.1002865},
volume = {9},
year = {2013}
}
