keywords = {Base Pairing,Evolution,Gene Expression Regulation,HeLa Cells,Humans,Messenger,Messenger: chemistry,Messenger: metabolism,MicroRNAs,MicroRNAs: chemistry,MicroRNAs: metabolism,Molecular,Probability,RNA,Software,Thermodynamics,miRNA,prediction,tool},
mendeley-tags = {miRNA,prediction,tool},
month = {dec},
number = {22},
pages = {11673--11683},
pmid = {23034802},
title = {{MiRmap: Comprehensive prediction of microRNA target repression strength}},
url = {http://nar.oxfordjournals.org/content/40/22/11673.full},
volume = {40},
year = {2012}
}
