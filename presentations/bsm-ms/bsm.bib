keywords = {Computational Biology,Genome, Human,Genome, Human: genetics,Humans,MicroRNAs,MicroRNAs: genetics,Polymorphism, Single Nucleotide,Polymorphism, Single Nucleotide: genetics,Selection, Genetic,evolution,miRNA},
mendeley-tags = {evolution,miRNA},
