month = {apr},
number = {7293},
pages = {1351--6},
pmid = {20428171},
publisher = {NIH Public Access},
title = {{Genome, epigenome and RNA sequences of monozygotic twins discordant for multiple sclerosis.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/20428171 https://www.ncbi.nlm.nih.gov/pmc/articles/PMC2862593/},
volume = {464},
year = {2010}
}
@article{Rabadan2015,
abstract = {In On the Origin of the Species in 1859, Charles Darwin proposed phylogenetic trees to depict evolutionary relationships on the basis of phenotypic attributes. Since then, biologists have shifted to using molecular traits to characterize evolutionary relationships between and among species. These approaches typically assume that traits are inherited vertically, and that evolution is strictly clonal. However, several other modes of genetic exchange add complexity to this process, including lateral gene transfers in bacteria, recombination and reassortment in viruses, viral integration in eukaryotes, and fusion of genomes of symbiotic species.},
