file = {:home/attila/bibliography/mendeley/Csan{\'{a}}dy et al/Csan{\'{a}}dy et al. - 2011 - Electrophysiological, Biochemical, and Bioinformatic Methods for Studying CFTR Channel Gating and Its Regulatio.pdf:pdf},
keywords = {review},
mendeley-tags = {review},
pages = {443--469},
