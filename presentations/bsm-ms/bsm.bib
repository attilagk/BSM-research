journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Disease Progression,Evolution, Molecular,Humans,Models, Biological,Mutation,Neoplasms,Neoplasms: genetics,Neoplasms: pathology,cancer,evolution},
mendeley-tags = {cancer,evolution},
