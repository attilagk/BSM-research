journal = {Nature communications},
keywords = {expression,genomics},
mendeley-tags = {expression,genomics},
pages = {11101},
pmid = {27089393},
title = {{A uniform survey of allele-specific binding and expression over 1000-Genomes-Project individuals.}},
url = {http://www.nature.com/ncomms/2016/160418/ncomms11101/full/ncomms11101.html?WT.ec{\_}id=NCOMMS-20160423 http://alleledb.gersteinlab.org/},
volume = {7},
year = {2016}
}
@article{ReyesHerrera2012,
abstract = {Nearly two decades have passed since the publication of the first study reporting the discovery of microRNAs (miRNAs). The key role of miRNAs in post-transcriptional gene regulation led to the performance of an increasing number of studies focusing on origins, mechanisms of action and functionality of miRNAs. In order to associate each miRNA to a specific functionality it is essential to unveil the rules that govern miRNA action. Despite the fact that there has been significant improvement exposing structural characteristics of the miRNA-mRNA interaction, the entire physical mechanism is not yet fully understood. In this respect, the development of computational algorithms for miRNA target prediction becomes increasingly important. This manuscript summarizes the research done on miRNA target prediction. It describes the experimental data currently available and used in the field and presents three lines of computational approaches for target prediction. Finally, the authors put forward a number of considerations regarding current challenges and future directions. {\textcopyright} 2012.},
author = {ReyesHerrera, Paula H. and Ficarra, Elisa},
