pages = {L57--9},
pmid = {18708465},
title = {{Coevolution of function and the folding landscape: correlation with density of native contacts.}},
url = {http://www.sciencedirect.com/science/article/pii/S0006349508785473},
volume = {95},
year = {2008}
}
