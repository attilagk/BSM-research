publisher = {Public Library of Science},
title = {{miRNA-mediated relationships between Cis-SNP genotypes and transcript intensities in lymphocyte cell lines}},
url = {http://dx.plos.org/10.1371/journal.pone.0031429},
volume = {7},
year = {2012}
}
